{
  "name": "coshub",
  "version": "1.0.0",
  "description": "二次元交流网站 - Monorepo",
  "private": true,
  "packageManager": "pnpm@9.15.9",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "dev:web": "turbo run dev --filter=@coshub/web",
    "dev:api": "turbo run dev --filter=@coshub/api",
    "dev:mini": "turbo run dev --filter=@coshub/mini",
    "build:web": "turbo run build --filter=@coshub/web",
    "build:api": "turbo run build --filter=@coshub/api",
    "build:mini": "turbo run build --filter=@coshub/mini",
    "build:packages": "turbo run build --filter=\"packages/*\"",
    "dev:all": "concurrently \"pnpm dev:web\" \"pnpm dev:api\"",
    "docker:start": "./scripts/docker-start.sh --dev",
    "docker:stop": "./scripts/docker-stop.sh",
    "install:all": "pnpm install",
    "legacy:start": "node legacy/index.js",
    "prepare": "husky",
    "commitlint": "commitlint --edit $GIT_PARAMS"
  },
  "keywords": [
    "二次元",
    "cosplay",
    "动漫",
    "社区",
    "交流",
    "monorepo"
  ],
  "author": "garrymei",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/garrymei/coshub.git"
  },
  "bugs": {
    "url": "https://github.com/garrymei/coshub/issues"
  },
  "homepage": "https://github.com/garrymei/coshub#readme",
  "devDependencies": {
    "@changesets/cli": "^2.29.6",
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.0.0",
    "husky": "^9.0.0",
    "prettier": "^3.0.0",
    "turbo": "^1.12.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "dependencies": {
    "@types/cache-manager": "^5.0.0",
    "cache-manager": "^7.2.0",
    "cache-manager-redis-store": "^3.0.1"
  }
}
