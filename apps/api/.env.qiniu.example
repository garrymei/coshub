NODE_ENV=development
PORT=3001

# 使用 七牛 Kodo（S3 兼容）作为对象存储
STORAGE_TYPE=qiniu
S3_ENDPOINT=https://s3.<region>.qiniucs.com
S3_ACCESS_KEY=<AK>
S3_SECRET_KEY=<SK>
S3_BUCKET=coshub-uploads
S3_REGION=<region>
S3_USE_SSL=true

# 上传限制
UPLOAD_MAX_FILE_SIZE=10485760
UPLOAD_ALLOWED_IMAGES=jpg,jpeg,png,gif,webp
UPLOAD_MAX_FILES=9
UPLOAD_PRESIGN_EXPIRES=3600

# CORS 允许源
CORS_ORIGINS=http://localhost:3000
