# Coshub 基础设施环境配置示例

# PostgreSQL 配置
POSTGRES_DB=coshub
POSTGRES_USER=coshub_user
POSTGRES_PASSWORD=coshub_password
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# Redis 配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=coshub_redis_password

# MinIO 配置
MINIO_ROOT_USER=coshub_minio_user
MINIO_ROOT_PASSWORD=coshub_minio_password
MINIO_HOST=localhost
MINIO_PORT=9000
MINIO_CONSOLE_PORT=9001

# PgAdmin 配置
PGADMIN_DEFAULT_EMAIL=admin@coshub.local
PGADMIN_DEFAULT_PASSWORD=pgadmin_password
PGADMIN_PORT=5050

# Redis Commander 配置
REDIS_COMMANDER_PORT=8081

# 网络配置
COMPOSE_PROJECT_NAME=coshub

# 开发环境覆盖（如果使用 --dev 参数）
DEV_POSTGRES_PASSWORD=dev123
DEV_REDIS_PASSWORD=dev123
DEV_MINIO_USER=dev123
DEV_MINIO_PASSWORD=dev123456
DEV_PGADMIN_PASSWORD=dev123
